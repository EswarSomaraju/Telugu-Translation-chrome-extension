{
  "manifest_version": 2,

  "name": "Telugu English Translation",
  "description": "This extension will translate English to Telugu",
  "version": "1.0",

  "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "English to Telugu"
    },

  "icons": {"128": "icon_128.png"},

  "permissions": [
   "activeTab"
   ]
}